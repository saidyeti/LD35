 cel_x_min=0,
 cel_x_max=15,
 cel_y_min=11,
 cel_y_max=15
}
land_obj_bounds = {
 cel_y_min=7,
 cel_y_max=9
}
water_obj_bounds = {
 cel_x_min=2,
 cel_x_max=17
}
fish_bounds = {
 cel_y_min=1,
 cel_x_max=14
}
player_bounds = {
 cel_x_min=1,
 cel_x_max=13,
 cel_y_min=6,
 cel_y_max=8
}
