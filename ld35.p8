}
maplength = 16
sky_cel_height = 7
house_cel_height = 2
beach_cel_height = 9
tilesize = 8
fish_spawn_probability = 0.15
water_obj_spawn_proability = 0.15
land_obj_spawn_probability = 0.15
water_blip_spawn_probability = 0.15
